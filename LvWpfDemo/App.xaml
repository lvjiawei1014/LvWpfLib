<Application x:Class="Ncer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="WindowStyleDemo.xaml">
    <Application.Resources>



        <DrawingBrush x:Key="TileBack" TileMode="Tile" Viewbox="0,0,16,16" Viewport="0,0,16,16" ViewportUnits="Absolute" ViewboxUnits="Absolute">
            <DrawingBrush.Drawing>
                <DrawingGroup>
                    <GeometryDrawing Brush="White" Geometry="M0,0L0,16 16,16 16,0z"/>
                    <GeometryDrawing Brush="#FF6D6D6D" Geometry="M0,0L0,8 8,8 8,0z"/>
                    <GeometryDrawing Brush="#FF6D6D6D" Geometry="M8,8L8,16 16,16 16,8z"/>
                </DrawingGroup>
            </DrawingBrush.Drawing>
        </DrawingBrush>





    </Application.Resources>
</Application>
